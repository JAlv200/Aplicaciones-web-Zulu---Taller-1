<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h5">¿Estás seguro que deseas cerrar sesión?</MudText>
        <MudText Typo="Typo.body2">Si cierras sesión, tendrás que volver a
            iniciar sesión para acceder a tu cuenta.</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Outlined" Color="Color.Tertiary" OnClick="CancelAction">Cancelar</MudButton>
        <MudSpacer/>
        <MudButton Variant="Variant.Filled" Color="Color.Error" OnClick="LogoutActionAsync">Cerrar Sesión</MudButton>
    </DialogActions>
</MudDialog>

